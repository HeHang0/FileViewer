<Window x:Class="FileViewer.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileViewer"
        mc:Ignorable="d" Icon="logo.ico"
        Title="关于" Height="300" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel>
            <Image Source="logo.ico" Width="50" Margin="0 30 0 0"></Image>
            <Label Width="89" FontWeight="Bold" FontSize="16" Margin="0 10 0 0">Previewer</Label>
            <Label Name="VersionText" Width="55" Margin="0 10 0 0">版本 1.0</Label>
            <Label Name="CopyrightText" Width="170" Margin="0 10 0 0">Copyright © 2019 oo__H__oo</Label>
            <Label Width="82" Margin="0 10 0 0">保留一切权利</Label>
        </StackPanel>
    </Grid>
</Window>
